{
  "webpackConfigJaid": "universalLib",
  "type": "module",
  "version": "2.0.6",
  "author": "Jaid <jaid.jsx@gmail.com> (https://github.com/Jaid)",
  "dependencies": {
    "forward-slash-path": "^0.3.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.22",
    "browserslist-config-jaid-universal": "^3.0.0",
    "eslint": "^9.36.0",
    "eslint-config-jaid": "^13.3.2",
    "rollup-config-factory": "^0.4.1",
    "tsconfig-jaid-node": "^8.1.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-config-jaid": "^17.4.2",
    "zeug": "^2.1.0"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "babel:prod": "rimraf dist/transpiled/production && NODE_ENV=production babel --out-dir dist/transpiled/production --source-maps true src",
    "babel:dev": "rimraf dist/transpiled/development && NODE_ENV=development babel --out-dir dist/transpiled/development src",
    "babel:test": "rimraf dist/transpiled/test && NODE_ENV=test babel --out-dir dist/transpiled/test src",
    "build:prod": "NODE_ENV=production webpack",
    "build:dev": "NODE_ENV=development webpack",
    "test": "bun test",
    "test:coverage": "npm run test:dev -- --coverage --json --outputFile=dist/jest/stats.json",
    "test:dev": "bun test",
    "test:debug": "DEBUG=\"$DEBUG,$(basename $PWD),$(basename $PWD):*\" NODE_ENV=test node --inspect=9229 node_modules/jest/bin/jest.js --runInBand",
    "prepareActionJest": "npm run build:prod"
  },
  "name": "zahl",
  "description": "Simplified wrapper for pluralize.",
  "funding": "https://github.com/sponsors/jaid",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaid/zahl"
  },
  "homepage": "https://github.com/Jaid/zahl",
  "keywords": [
    "format",
    "lib",
    "library",
    "node",
    "node-js",
    "plural",
    "pluralize",
    "string",
    "util",
    "utility"
  ]
}